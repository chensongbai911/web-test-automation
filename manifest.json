{
  "manifest_version": 3,
  "name": "Web功能自动化测试工具",
  "version": "2.0.0",
  "description": "智能自动化测试 + 自定义测试用例 - 支持上传JSON测试文档，无需登录注册",
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest",
    "tabs",
    "storage",
    "webNavigation",
    "unlimitedStorage",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "src/popup.html",
    "default_title": "AI智能测试工具"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "src/extension-context-checker.js",
        "src/e2e-scenario-tracker.js",
        "src/test-case-parser.js",
        "src/custom-test-executor.js",
        "src/test-flow-manager.js",
        "src/test-execution-optimizer.js",
        "src/ai-analysis-enhancer.js",
        "src/qwen-integration.js",
        "src/ai-intent-engine.js",
        "src/ai-test-orchestrator.js",
        "src/enhanced-test-reporter.js",
        "src/ai-form-analyzer.js",
        "src/complex-form-handler.js",
        "src/form-autofiller.js",
        "src/form-handlers.js",
        "src/filter-handlers.js",
        "src/floating-ball-injector.js",
        "src/content-script.js"
      ],
      "css": ["src/floating-ball.css"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/floating-ball.js",
        "src/custom-test-executor.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
